{% for dataset in DATASETS %}
<tr>
	<td class="text-center">
		<span class="btn-link" data-ldn="{{dataset.LDN}}">
			<i class="fa fa-trash"></i>
		</span>
	</td>
	<td class="text-left">
		<span data-toggle="tooltip" data-title="prodsysStatus: {{dataset.PRODSYSSTATUS}}, completion: {{dataset.COMPLETION}}, amiStatus: {{dataset.AMISTATUS}}">
			<i class="fa fa-question-circle"></i>
		</span>
		<span class="badge badge-{{dataset.COLOR}}">
			{{dataset.STATUS}}
		</span>
	</td>
	<td class="text-left">
		{{dataset.LDN}}
	</td>
</tr>
{% endfor %}