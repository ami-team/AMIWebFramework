<div class="table-responsive table-condensed" style="transform:rotateX(180deg); -ms-transform:rotateX(180deg); -webkit-transform:rotateX(180deg);">
	<table class="table table-striped table-bordered" id="ami_table" style="transform:rotateX(180deg); -ms-transform:rotateX(180deg); -webkit-transform:rotateX(180deg);">
		<tr>
			<th></th>
			{% if rows is not empty %}
			{% for field in rows[0].field %}
			{% if field['@name'] !== 'PROJECT' and field['@name'] !== 'PROCESS' and field['@name'] !== 'AMIENTITYNAME' and field['@name'] !== 'AMIELEMENTID' and field['@name'] !== 'AMICREATED' and field['@name'] !== 'AMILASTMODIFIED' and field['@name'] !== 'AMISYSDATE' %}
			<th>
				<a href="#" onclick="amiTableApp.sortTable('DESC','{{field['@name']}}')"><i class="fa fa-chevron-down"></i></a>
				{{field['@name']}} 
				<a href="#" onclick="amiTableApp.sortTable('ASC','{{field['@name']}}')"><i class="fa fa-chevron-up"></i></a>
				<br />
				<a href="#"><i class="fa fa-search amitt amipo" id="ami_table_filter" data-toggle="popover" data-title="Apply filter" data-content="&lt;div class=&quot;form-group&quot;&gt;&lt;label class=&quot;control-label&quot; for=&quot;ami_table_filter_type{{loop.index0}}&quot;&gt;{{field['@table'] ~ '.' ~ field['@name']}}&lt;/label&gt;&lt;select class=&quot;form-control &quot; id=&quot;ami_table_filter_type{{loop.index0}}&quot;&gt;&lt;option value=&quot;like&quot;&gt;LIKE&lt;/option&gt;&lt;option value=&quot;notLike&quot;&gt;NOT LIKE&lt;/option&gt;&lt;option value=&quot;equal&quot;&gt;=&lt;/option&gt;&lt;option value=&quot;different&quot;&gt;!=&lt;/option&gt;&lt;option value=&quot;superior&quot;&gt;&gt;&lt;/option&gt;&lt;option value=&quot;superiorEqual&quot;&gt;&gt;=&lt;/option&gt;&lt;option value=&quot;inferior&quot;&gt;&lt;&lt;/option&gt;&lt;option value=&quot;inferiorEqual&quot;&gt;&lt;=&lt;/option&gt;&lt;/select&gt;&lt;/div&gt;&lt;div class=&quot;form-group&quot;&gt;&lt;label class=&quot;control-label&quot; for=&quot;ami_table_filter_value{{loop.index0}}&quot;&gt;value&lt;/label&gt;&lt;input type=&quot;text&quot; class=&quot;form-control&quot; id=&quot;ami_table_filter_value{{loop.index0}}&quot; /&gt;&lt;/div&gt;&lt;div class=&quot;text-right&quot;&gt;&lt;button class=&quot;btn btn-primary&quot; onclick=&quot;amiTableApp.submitFilter('{{field['@table'] ~ '.' ~ field['@name']}}',$('#ami_table_filter_type{{loop.index0}} option:selected').text(),$('#ami_table_filter_value{{loop.index0}}').val())&quot; &gt;Submit&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;" data-placement="bottom"></i></a>
				{% if field['@type'] starts with 'NUMBER' %}
				&nbsp;<a href="#" onclick="amiTableApp.stats('{{field['@name']}}')"><i class="fa fa-picture-o amitt" data-toggle="tooltip" data-title="Stats"></i></a>
				{% endif %}
				&nbsp;<a href="#" onclick="amiTableApp.groupBy('{{field['@name']}}')"><i class="fa fa-th amitt" data-toggle="tooltip" data-title="Group by"></i></a>
			</th>
			{% endif %}
			{% endfor %}
		</tr>
		{% for row in rows %}

			{% for field in row.field %}
				{% if field['@name'] === 'AMIELEMENTID' %}{% set elementId = field.$ %}{% endif %}
			{% endfor %}

		<tr>
			
			<td>
				<a href="#"onclick="amiTableApp.details('{{elementId}}')">details</a>&nbsp;&nbsp;<a><i class="fa fa-arrows-alt fa-lg"></i></a>
				{% for field in row.field %}
				{% if field['@name'] === 'created' %}
				{% endif %}
			</td>
			
			{% if field['@name'] !== 'PROJECT' and field['@name'] !== 'PROCESS' and field['@name'] !== 'AMIENTITYNAME' and field['@name'] !== 'AMIELEMENTID' and field['@name'] !== 'AMICREATED' and field['@name'] !== 'AMILASTMODIFIED' and field['@name'] !== 'AMISYSDATE' %}
			<td>
				{% if field.properties is defined %}
				{% set property = field.properties[0] %}
				<span class="editable {{property['@class']}}" id="{{property['@id']}}" style="{{property['@css']|escape('html')}}" script="{{property['@javascript']|escape('html')}}" data-unitName="{{property['@unitName']}}" data-unitFactor="{{property['@unitFactor']}}" data-unitBase="{{property['@unitBase']}}" data-humanReadable="{{property['@humanReadable']}}" data-col="{{field['@name']}}" data-row="{{elementId}}" placeholder="&nbsp;">{{field.$|default('N/A')}}</span>
				{% else %}
				<span class="editable" data-col="{{field['@name']}}" data-row="{{elementId}}" placeholder="&nbsp;">{{field.$|default('N/A')}}</span>
				{% endif %}
				<br />
			
				{% if field.link is defined %}
				{% for link in field.link %}
				{% if not loop.first %}
				<span> - </span>
				{% endif %}
				<a class="{{link['@class']}}" id="{{link['id']}}" style="{{link['@css']|escape('html')}}" script="{{link['@javascript']|escape('html')}}" href="{{link['@href']|replace({'executeLinkedAMICommandTab(': 'executeLinkedAMICommandTab(\'' + link['@label'] + '\','})|escape('html')}}" target="{{link['@target']}}">{{link['@label']}}</a>
				{# {{link['@href'] | escape('js') | escape('html')}} #}
				{% endfor %}
				{% endif %}
			</td>
			{% endif %}
			{% endfor %}
		</tr>
		{% endfor %}
		{% endif %}
	</table>
</div>
